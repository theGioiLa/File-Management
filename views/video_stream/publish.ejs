<% 
let i = 0;
for (i = 0; i < channels.length; i += 2) { %>
    <div class="row">
        <div class="col">
            <div id="player<%-i%>" channel="<%-channels[i]%>"></div>
        </div>
        <div class="col">
            <div id="player<%-i+1%>" channel="<%-channels[i+1]%>"></div>
        </div>
    </div>
    <br>
<%} %>

<script type="text/javascript" src="/jwplayer/jwplayer.js"></script>
<script>
    let nChannels = Number("<%-channels.length%>");
    let rtmpServer = "<%-rtmpServer%>";
    for (let i = 0; i < nChannels; i++) {
        let player = jwplayer(`player${i}`);
        let channel = $(`#player${i}`).attr("channel");
        console.log(channel);

        player.setup({
            sources: [{
                file: `${rtmpServer}/${channel}`,
            }],
        })
    }
</script>